{
  "name": "automapper-add-on",
  "description": "Add-on for @automapper",
  "private": true,
  "scripts": {
    "prepare": "husky install || true",
    "preinstall": "npx only-allow pnpm",
    "clear": "pnpm -r exec -- rm -rf",
    "clear-node-modules": "pnpm -r exec -- rm -rf node_modules",
    "update-packages": "pnpm update -i -r --latest"
  },
  "keywords": [
    "automapper",
    "mapper",
    "automapper-add-on",
    "mapper-add-on"
  ],
  "author": "Alpha",
  "repository": {
    "type": "git",
    "url": "git@github.com:zgid123/automapper-add-on.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^3.25.1",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@automapper/core": "^8.7.7",
    "tslib": "^2.5.3"
  }
}
