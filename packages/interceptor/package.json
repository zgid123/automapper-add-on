      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "prepublish": "pnpm build",
    "build": "rollup --config rollup.config.ts --configPlugin typescript"
  },
  "dependencies": {
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@nestjs/common": "^10.0.1",
    "rxjs": "^7.8.1"
  }
}
