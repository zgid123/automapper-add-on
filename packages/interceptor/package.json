{
  "name": "@automapper-add-on/interceptor",
  "version": "1.0.1",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "author": "Alpha",
  "description": "NestJS Interceptor for @automapper",
  "homepage": "https://github.com/zgid123/automapper-add-on",
  "keywords": [
    "automapper",
    "mapper",
    "automapper-add-on",
    "mapper-add-on",
    "automapper-interceptor",
    "mapper-interceptor",
    "automapper-add-on-interceptor",
    "mapper-add-on-interceptor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zgid123/automapper-add-on"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
